#include <stdio.h>
int main()
{
	int num1,num2,num3;
	float f1,f2,f3;
	char ch1,ch2,ch3;
	
	printf("\nEnter the value of num1 = ");
	scanf("%d",&num1);
	printf("\nEnter the value of num2 = ");
	scanf("%d",&num2);
	printf("\nEnter the value of num3 = ");
	scanf("%d",&num3);
	
	printf("\n%d",num1);
	printf("\n%d",num2);
	printf("\n%d",num3);
	
	printf("\nEnter the value of f1 = ");
	scanf("%f",&f1);
	printf("\nEnter the value of f2 = ");
	scanf("%f",&f2);
	printf("\nEnter the value of f3 = ");
	scanf("%f",&f3);
	
	printf("\n%f",f1);
	printf("\n%f",f2);
	printf("\n%f",f3);
	
	printf("\nEnter the value of ch1 = ");
	scanf("%d",&ch1);
	printf("\nEnter the value of ch2 = ");
	scanf("%d",&ch2);
	printf("\nEnter the value of ch3 = ");
	scanf("%d",&ch3);
	
	printf("\n%c",ch1);
	printf("\n%c",ch2);
	printf("\n%c",ch3);
	
	return 0;
}
